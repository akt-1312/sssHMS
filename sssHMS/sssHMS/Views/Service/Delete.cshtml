@model sssHMS.Models.Service
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Service Create</h1>
<form method="post">
    <div asp-validation-summary="ModelOnly"></div>
    <input hidden name="returnUrl" value="@ViewBag.returnUrl" />
    <input hidden asp-for="ServiceID" />

    <div class="form-group col-md-5">
        <label asp-for="ServiceName" class="control-label"></label>
        <input readonly id="CountryName" asp-for="ServiceName" autocomplete="off" class="form-control" />
        @*<span asp-validation-for="ServiceName" class="text-danger"></span>*@
    </div>


    <div class="form-group col-md-5">
        <button type="submit" id="btnSave" class="btn btn-success">Delete</button>

        <a class="btn btn-primary" href="@ViewBag.returnUrl">Back To Main</a>
    </div>


</form>

